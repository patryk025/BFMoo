OBJECT=ARR
ARR:TYPE=ARRAY

OBJECT=TOP
TOP:TYPE=INTEGER
TOP:VALUE=-1

OBJECT=PUSH
PUSH:TYPE=BEHAVIOUR
PUSH:CODE={ARR^ADD($1);TOP^INC();}

OBJECT=POP
POP:TYPE=BEHAVIOUR
POP:CODE={@RETURN(ARR^GET(TOP));ARR^REMOVEAT(TOP);TOP^DEC();}

OBJECT=PEAK
PEAK:TYPE=BEHAVIOUR
PEAK:CODE={@RETURN(ARR^GET(TOP));}

OBJECT=ISEMPTY
ISEMPTY:TYPE=BEHAVIOUR
ISEMPTY:CODE={@IF("TOP","<","0","{@RETURN(TRUE);}","{@RETURN(FALSE);}");}

OBJECT=CONSTRUCTOR
CONSTRUCTOR:TYPE=BEHAVIOUR
CONSTRUCTOR:CODE={ARR^REMOVEALL();TOP^SET(-1);}
